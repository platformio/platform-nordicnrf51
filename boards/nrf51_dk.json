{
  "build": {
    "arduino":{
      "ldscript": "nrf51_xxaa.ld"
    },
    "core": "nRF5",
    "cpu": "cortex-m0",
    "extra_flags": "-DPCA10000",
    "f_cpu": "32000000L",
    "mcu": "nrf51822",
    "variant": "PCA1000X",
    "zephyr": {
       "variant": "nrf51dk_nrf51422"
    }
  },
  "connectivity": [
    "bluetooth"
  ],
  "debug": {
    "default_tools": [
      "jlink"
    ],
    "jlink_device": "nRF51802_xxAA",
    "onboard_tools": [
      "cmsis-dap",
      "jlink"
    ],
    "svd_path": "nrf51.svd"
  },
  "frameworks": [
    "arduino",
    "zephyr"
  ],
  "name": "Nordic nRF51X22 Development Kit(PCA1000X)",
  "upload": {
    "maximum_ram_size": 32768,
    "maximum_size": 262144,
    "protocol": "jlink",
    "protocols": [
      "cmsis-dap",
      "blackmagic",
      "jlink",
      "nrfjprog",
      "stlink"
    ]
  },
  "url": "https://www.nordicsemi.com/Software-and-Tools/Development-Kits/nRF51-DK",
  "vendor": "Nordic"
}
